<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.retaliator.carex">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DetalleRepostaje"
            android:label=""
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".DetalleMantenimiento"
            android:label=""
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".NuevoRepostaje"
            android:label=""
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".NuevoMantenimiento"
            android:label=""
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".CarsListActivity"
            android:label=""
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.retaliator.carex.MainActivity" />
        </activity>
        <activity
            android:name=".NuevoCoche"
            android:label=""
            android:parentActivityName=".CarsListActivity" />
        <activity
            android:name=".DetalleCoche"
            android:label=""
            android:parentActivityName=".CarsListActivity" />
        <activity android:name=".Graficos" />
        <activity android:name=".SearchActivity"
            android:label="Buscar..."
            android:parentActivityName=".MainActivity">
        </activity>
    </application>

</manifest>