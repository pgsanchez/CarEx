<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity_informes2">

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:showDividers="beginning|end">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_horizontal">

            <TextView
                android:id="@+id/Celda00"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="12dp"
                android:text="Cabecera 00"
                android:textColor="#550000"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/Celda01"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="5px"
                android:text="Cabecera 01"
                android:textColor="#005500"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/Celda02"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="5px"
                android:text="Cabecera 02"
                android:textColor="#005500"
                android:textSize="18sp" />
        </TableRow>

        <TableRow
            android:id="@+id/SeparadorCabecera"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal">

            <FrameLayout
                android:id="@+id/LineaCabecera"
                android:layout_width="match_parent"
                android:layout_height="6px"
                android:background="#220022"
                android:layout_span="3"
                tools:layout_width="match_parent"></FrameLayout>
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_horizontal">

            <TextView
                android:id="@+id/Celda10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="5px"
                android:text="Hola"
                android:textColor="#005500"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/Celda11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="5px"
                android:text="1234"
                android:textColor="#005500"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/Celda12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="5px"
                android:text="Caracola"
                android:textColor="#005500"
                android:textSize="18sp" />
        </TableRow>


        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_horizontal">

            <TextView
                android:id="@+id/Celda20"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="5px"
                android:text="Hola"
                android:textColor="#005500"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/Celda21"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="5px"
                android:text="1234"
                android:textColor="#005500"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/Celda22"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="5px"
                android:text="Caracola"
                android:textColor="#005500"
                android:textSize="18sp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_horizontal">

            <TextView
                android:id="@+id/Celda30"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="5px"
                android:text="Hola"
                android:textColor="#005500"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/Celda31"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="5px"
                android:text="1234"
                android:textColor="#005500"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/Celda32"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="5px"
                android:text="Caracola"
                android:textColor="#005500"
                android:textSize="18sp" />
        </TableRow>
    </TableLayout>
</android.support.constraint.ConstraintLayout>